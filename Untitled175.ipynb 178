        "\n",
        "HOST = 'localhost'\n",
        "PORT = 7777\n",
        "def echoclient2():\n",
        "  with socket.socket(socket.AF_INFT,socket.SOCK_STREAM) as sock:\n",
        "    sock.connect((HOST,PORT))\n",
        "\n",
        "    while True:\n",
        "      msg = input('메시지 입력:')\n",
        "      if msg == '/q':\n",
        "        sock.sendall(msg.encode())\n",
        "        break\n",
        "\n",
        "      sock.sendall(msg.encode())\n",
