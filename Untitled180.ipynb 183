      "source": [
        "def cumulativeSum(n):\n",
        "  ret = 0\n",
        "  print(f'{n:,}까지 누적합 계산중...')\n",
        "  stime = time()\n",
        "  for i in range(1,n+1):\n",
        "    ret += i\n",
        "  etime = time()\n",
        "  print(f'{n:,}까지 누적합 계산 시간{etime-stime:.2f}초')\n",
        "  print(f'누적합 결과:{ret:,}')\n",
        "  return ret"
      ],
