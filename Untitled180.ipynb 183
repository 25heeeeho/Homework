        "fro time import time\n",
        "\n",
        "def checkPrimeNumber(x):\n",
        "  ret = False\n",
        "\n",
        "  if x==2:\n",
        "    return True\n",
        "  if x<=1 or x%2==0:\n",
        "    return False\n",
        "\n",
        "  print(f'{x:,}가 소수인지 아닌지 판별 중...')\n",
        "  stime = time()\n",
        "  for i in range(3,int(x**(1/2))+1,2):\n",
        "    if x%i==0:\n",
        "      ret = False\n",
        "  else:\n",
        "    ret = True\n",
        "  etime = time()\n",
        "\n",
        "  if ret:\n",
        "    print(f'{x:}는 소수입니다. 소요 시간:{etime-stime:.2f}초')\n",
        "  else:\n",
        "    print(f'{x:,}는 소수가 아닙니다.')\n",
        "\n",
        "  return ret"
      ]
