        "import socket\n",
        "\n",
        "HOST = ''\n",
        "PORT = 7777\n",
        "\n",
        "def echoServer():\n",
        "  with socket.socket(socket.AF_INFT,socket.SOCK_STREAM) as sock:\n",
        "    sock.bind((HOST,PORT))\n",
        "    sock.listen()\n",
        "    print('클라이언트 연결을 기다리는 중...')\n",
        "    conn,addr = sock.accept()\n",
        "    with conn:\n",
        "      print(f'{addr[0]}와 연결됨')\n",
        "      while True:\n",
        "        data = conn.recv(1024)\n",
        "        if not data:\n",
        "          break\n",
        "          print(f'수신 메시지:{data.decode()}')\n",
